{
  "compilerOptions": {
    "rootDir": "./",
    "lib": ["ES2023"],
    "module": "EsNext",
    "moduleResolution": "node",
    "target": "ES2022",
    "checkJs": false,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strict": true,
    "strictFunctionTypes": true,
    "types": ["node"],
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noUncheckedIndexedAccess": true
  }
}
